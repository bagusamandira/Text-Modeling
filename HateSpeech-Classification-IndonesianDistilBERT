      "cell_type": "code",
      "source": [
        "def tokenize_inputs(texts, tokenizer, max_length):\n",
        "    tokenized = tokenizer.batch_encode_plus(\n",
        "        texts.tolist(),\n",
        "        max_length=max_length,\n",
        "        pad_to_max_length=True,\n",
        "        truncation=True,\n",
        "        return_token_type_ids=False,\n",
        "        return_attention_mask=True,\n",
        "        return_tensors='pt'\n",
        "    )\n",
        "    return tokenized\n",
        "\n",
        "train_encodings = tokenize_inputs(x_train, dbert_tokenizer, 32)\n",
        "val_encodings = tokenize_inputs(x_val, dbert_tokenizer, 32)\n",
        "test_encodings = tokenize_inputs(x_test, dbert_tokenizer, 32)"
      ],
      "metadata": {
        "id": "4Q7rtzJriMED",
        "colab": {
          "base_uri": "https://localhost:8080/"
